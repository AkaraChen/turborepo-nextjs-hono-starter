{
  "name": "turborepo-nextjs-hono-starter",
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "lefthook": "^1.7.11",
    "syncpack": "^12.4.0",
    "turbo": "^2.0.11",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@8.15.9",
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "19",
        "react-dom": "19"
      }
    },
    "overrides": {
      "react": "19.0.0-rc-f994737d14-20240522",
      "react-dom": "19.0.0-rc-f994737d14-20240522"
    }
  },
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "format": "biome format . --write",
    "lint": "turbo lint",
    "prepare": "lefthook install"
  }
}
